{
  "name": "photo-search-ui",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run build:app && npm run build:zip && npm run build:dist",
    "build:app": "GENERATE_SOURCEMAP=false INLINE_RUNTIME_CHUNK=false react-scripts build",
    "build:zip": "cd build && zip -rq ../ui.zip . && cd ..",
    "build:dist": "mv ui.zip build/",
    "test": "react-scripts test --watchAll=false",
    "test:watch": "react-scripts test",
    "type-check": "tsc --pretty --noEmit",
    "lint": "run-p -l -c --aggregate-output lint:*",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "lint:css": "stylelint \"src/**/*.scss\"",
    "lint:ts": "npm run type-check",
    "fix": "run-s fix:css fix:prettier fix:eslint",
    "fix:eslint": "eslint . --fix",
    "fix:prettier": "prettier . --write",
    "fix:css": "stylelint \"src/**/*.scss\" --fix",
    "clean": "run-p -l -c --aggregate-output clean:*",
    "clean:modules": "rm -rf node_modules package-lock.json"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^1.0.5",
    "@awsui/components-react": "^3.0.367",
    "@awsui/design-tokens": "^3.0.7",
    "@awsui/global-styles": "^1.0.7",
    "aws-amplify": "^4.3.3",
    "i18next": "^19.9.2",
    "i18next-browser-languagedetector": "^6.0.1",
    "i18next-http-backend": "^1.1.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.8.10",
    "react-router-dom": "^5.2.0",
    "recoil": "^0.1.3",
    "uuid": "^8.3.2",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.1",
    "@types/react-router-dom": "^5.1.7",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "react-scripts": "4.0.3",
    "sass": "^1.32.8",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.19.0",
    "typescript": "^4.1.2"
  }
}
